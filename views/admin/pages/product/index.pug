extends ../../layouts/default.pug


block main
    h1 Trang danh sách sản phẩm

    table.table.table-hover.table-sm
        thead
            tr
                th STT
                th Hình ảnh
                th Tiêu đề
                th Giá
                th Trạng thái
                th Hành động
        tbody
            tr
                td 1
                td
                    img(src="data:image/jpeg;base64,"
                        alt="Iphone 17" width="100" height="auto")
                td Sản phẩm 1
                td $100
                td
                    a.badge.bg-success(href="#") Hoạt động
                td
                    button.btn.btn-warning.btn-sm Sửa
                    button.btn.btn-danger.btn-sm.ml-1 Xóa

            tr
                td 1
                td
                    img(src="data:image/jpeg;base64,"
                        alt="Iphone 17" width="100" height="auto")
                td Sản phẩm 1
                td $100
                td
                    a.badge.bg-danger(href="#") Dừng hoạt động
                td
                    button.btn.btn-warning.btn-sm Sửa
                    button.btn.btn-danger.btn-sm.ml-1 Xóa
